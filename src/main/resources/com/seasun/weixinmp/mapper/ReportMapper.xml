<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.seasun.weixinmp.mapper.ReportMapper">  

		<select id="test" resultType="ClassInfo">
        select class_id, class_name, `desc` from class_info
    </select>
    
    <select id="getCardInfoById" resultType="CardInfo">
        select * from card_info where card_id = #{cardId} limit 1
    </select>
    
    

</mapper>